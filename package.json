{
  "name": "@yanzisuka/storybook-preset-craco",
  "version": "1.0.2",
  "packageManager": "yarn@4.0.2",
  "description": "One-line CRACO configuration for Storybook",
  "homepage": "https://github.com/YanZisuka/storybook-preset-craco",
  "bugs": {
    "url": "https://github.com/YanZisuka/storybook-preset-craco/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YanZisuka/storybook-preset-craco.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && tsc"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "@craco/craco": ">=7",
    "@storybook/node-logger": "*",
    "@storybook/react": ">=6",
    "@storybook/react-docgen-typescript-plugin": ">=1",
    "react-scripts": ">=5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@craco/craco": "^7.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/node-logger": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^4.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,md}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
